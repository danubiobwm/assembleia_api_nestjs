{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true
  },
  "generateOptions": {
    "spec": false
  },
  "spec": {
    "class": false,
    "component": false,
    "module": false,
    "service": false,
    "controller": false
  }
}
